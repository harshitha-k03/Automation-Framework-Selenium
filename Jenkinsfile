pipeline { agent any stages { stage('Checkout') { steps { git 'https://github.com/your-username/SeleniumAutomationFramework.git' } } stage('Build') { steps { sh 'mvn clean install' } } stage('Test') { steps { sh 'mvn test' } } stage('Generate Reports') { steps { allure([ includeProperties: false, jdk: '', results: [[path: 'target/allure-results']] ]) } } } post { always { archiveArtifacts artifacts: 'reports/.html', allowEmptyArchive: true archiveArtifacts artifacts: 'test-recordings/.avi', allowEmptyArchive: true } } }